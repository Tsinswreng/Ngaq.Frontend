<Project>
	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<PublishAot>true</PublishAot>
		<PublishTrimmed>true</PublishTrimmed>
		<NoWarn>CS1998</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="../../../Tsinswreng.CsTypeAlias/TypeAlias.cs"/>
	</ItemGroup>

	<ItemGroup>
		<Compile Include="../Tsinswreng.Avalonia/GlobalUsing.cs"/>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="../../../Tsinswreng.CsCore/Tsinswreng.CsCore.csproj" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="../../../Ngaq.Core/Ngaq.Core.csproj" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection"/>
	</ItemGroup>
</Project>
